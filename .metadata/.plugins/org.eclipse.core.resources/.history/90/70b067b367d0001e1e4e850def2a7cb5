package daily_0221;

import java.io.*;
import java.util.*;

public class SW_3289_서로소집합 {
	

	public static void makeSet(int[] parent,int n) {
		for(int i=1;i<=n;i++) {
			parent[i]=i;
		}
		
	}
	
