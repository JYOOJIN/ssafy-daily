package daily_0419;

import java.io.*;
import java.util.*;

public class BJ_2470_두용액 {

	public static void main(String[] args) throws Exception{
		BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st;
		int n=Integer.parseInt(br.readLine());
		int[] map=new int[n];
		
		st=new StringTokenizer(br.readLine());
		for(int i=0;i<n;i++) {
			map[i]=Integer.parseInt(st.nextToken());
		}
		
		Arrays.sort(map);
		
		int start=0;
		int end=n-1;
		int result=map[start]+map[end]; 
		int min=Math.abs(result);
		
		while(true) {
			if(Math.abs(result)>min) break;
			if(Math.abs(result)<min) min=Math.abs(result);
			
			if(result<0) { //0보다 작으면 end를 움직인다
				end-=1;
			}else if(result>0) { //0보다 크면 start를 움직인다
				start+=1;
			}else { 
				break;
			}
			
			result=map[start]+map[end];
		}
		System.out.println(map[start]+" "+map[end]);
		return;
		
	}

}
